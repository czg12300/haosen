	<TextView
          android:id="@+id/timingsetDialog_title"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginLeft="5dp"
          android:layout_marginTop="10dp"
          android:text="定时设置"
          android:textColor="#ff6e01"
          android:textSize="18sp" />
    <RelativeLayout
        android:id="@+id/fragment_timingsetDialog_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="45dp"
        >

    <LinearLayout android:layout_width="wrap_content" 
        android:layout_height="wrap_content" 
        android:gravity="center"
        android:layout_marginTop="165dp"
        >

        <Button
            android:id="@+id/timingsetDialog_submit"
            android:layout_width="110dp"            
            android:layout_height="wrap_content"
            android:background="@drawable/selector_btnbg"
            android:text="设定" 
            android:textColor="#ffffff"
            android:textSize="18sp" 
            android:layout_margin="5dp"/>

        <Button
            android:id="@+id/timingsetDialog_cancel"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:background="@drawable/selector_btnbg"
            android:text="取消" 
            android:textColor="#ffffff"
            android:textSize="18sp" 
            android:layout_margin="5dp"
            />

    </LinearLayout>
    
    
    </RelativeLayout>

    
        
</RelativeLayout>